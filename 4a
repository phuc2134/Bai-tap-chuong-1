#include <iostream>

using namespace std;

int main() {
    int quantity;
    float unit_price;
    float total, vat;

    cout << "Nhap vao so luong: ";
    cin >> quantity;

    cout << "Nhap vao don gia: ";
    cin >> unit_price;

    total = quantity * unit_price;
    vat = total * 0.1;

    cout << "Tien hang: " << total << endl;
    cout << "Thue gia tri gia tang (VAT): " << vat << endl;
    cout << "Tong tien phai tra: " << total + vat << endl;

    return 0;
    }
